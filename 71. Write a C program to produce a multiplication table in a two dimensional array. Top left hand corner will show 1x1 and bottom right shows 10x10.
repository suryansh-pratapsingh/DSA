#include <stdio.h>

int main() {
    int table[10][10]; // Create a 10x10 array to store the multiplication table

    // Populate the array with multiplication results
    for (int i = 1; i <= 10; i++) {
        for (int j = 1; j <= 10; j++) {
            table[i ][j] = i * j;
        }
    }

    // Display the multiplication table
    printf("Multiplication Table:\n");
    for (int i = 0; i < 10; i++) {
        for (int j = 0; j < 10; j++) {
            printf("%3d\t", table[i][j]); // Display each element with 3 spaces
        }
        printf("\n"); // Move to the next row
    }

    return 0;
}
