#include <stdio.h>

int main() {
    int size;
    int a[size],i,sum=0;
    printf("\nentrer no. of digits");
    scanf("%d",&size);
    printf("\nenter digits\n");
    for(i=0;i<size;i++){
        scanf("%d",&a[i]);
    }
    for(i=0;i<size;i++){
        sum=sum+a[i];
        
    }
    printf("%d",sum);
 
return 0;
}
