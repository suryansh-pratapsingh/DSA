#include <stdio.h>

int main() {
    int n = 5;

    // a.
    printf("a.\n");
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= i; j++) {
            printf("*");
        }
        printf("\n");
    }

    // b.
    printf("\nb.\n");
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= i; j++) {
            printf("%d", j);
        }
        printf("\n");
    }

    // c.
    printf("\nc.\n");
    for (int i = 1; i <= n; i++) {
        char ch = 'A';
        for (int j = 1; j <= i; j++) {
            printf("%c", ch++);
        }
        printf("\n");
    }

    // d.
    printf("\nd.\n");
    int num = 1;
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= i; j++) {
            printf("%d", num++);
        }
        printf("\n");
        num -= i;
        num += 10;
    }

    // e.
    printf("\ne.\n");
    for (int i = n; i >= 1; i--) {
        for (int j = 1; j <= i; j++) {
            printf("*");
        }
        printf("\n");
    }

    // f.
    printf("\nf.\n");
    for (int i = n; i >= 1; i--) {
        char ch = 'A';
        for (int j = 1; j <= i; j++) {
            printf("%c", ch++);
        }
        printf("\n");
    }

    // g.
    printf("\ng.\n");
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= 2 * i - 1; j++) {
            printf("*");
        }
        printf("\n");
    }

    // h.
    printf("\nh.\n");
    int k = 1;
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= i; j++) {
            printf("%d", k++);
        }
        printf("\n");
    }

    // i.
    printf("\ni.\n");
    for (int i = n; i >= 1; i--) {
        for (int j = 1; j <= i; j++) {
            printf("*");
        }
        printf("\n");
    }

    // j.
    printf("\nj.\n");
    for (int i = n; i >= 1; i--) {
        char ch = 'A';
        for (int j = 1; j <= i; j++) {
            printf("%c", ch++);
        }
        printf("\n");
    }

    // l.
    printf("\nl.\n");
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= i; j++) {
            printf("%d", j);
        }
        printf("\n");
    }

    // m.
    printf("\nm.\n");
    for (int i = n; i >= 1; i--) {
        for (int j = 1; j <= i; j++) {
            printf("*");
        }
        for (int j = 1; j <= 4 * (n - i); j++) {
            printf("0");
        }
        printf("\n");
    }

    // n.
    printf("\nn.\n");
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= i; j++) {
            printf("%d", (j % 2));
        }
        printf("\n");
    }

    // o.
    printf("\no.\n");
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= i; j++) {
            printf("%d", i);
        }
        printf("\n");
    }

    // p.
    printf("\np.\n");
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= i; j++) {
            printf("%d", j);
        }
        for (int j = i - 1; j >= 1; j--) {
            printf("%d", j);
        }
        printf("\n");
    }

    // q.
    printf("\nq.\n");
    for (int i = n; i >= 1; i--) {
        char ch = 'A';
        for (int j = 1; j <= i; j++) {
            printf("%c", ch++);
        }
        for (int j = i; j < n; j++) {
            printf(" ");
        }
        ch--;
        for (int j = i; j >= 1; j--) {
            printf("%c", --ch);
        }
        printf("\n");
    }

    return 0;
}
