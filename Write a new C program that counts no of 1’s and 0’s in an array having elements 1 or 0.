#include <stdio.h>
int main() {
    int i,count=0,ones=0,n,k;
    printf("entr size of array");
    scanf("%d",&n);
    int a[n];
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++){
        if(a[i]==0){
            count++;
        }
        else if(a[i]==1){
            ones++;
        }
    }
    printf("no of zero %d",count);
    printf("no of ones %d",ones);
    return 0;
}
