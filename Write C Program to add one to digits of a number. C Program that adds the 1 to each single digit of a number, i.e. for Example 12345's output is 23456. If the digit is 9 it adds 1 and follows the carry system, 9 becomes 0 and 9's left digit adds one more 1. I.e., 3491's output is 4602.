#include <stdio.h>

int main() {
    int n,r,sum=0;
    printf("enter");
    scanf("%d",&n);
        while(n>0){
            r=n%10;
            r++;
            n=n/10;
            sum=(sum*10)+r;
        }
        n=sum;
        sum=0;
         while(n>0){
            r=n%10;
            printf("%d",r);
            n=n/10;
            sum=(sum*10)+r;
         }
    return 0;
}
