#include <stdio.h>

int main() {
    int a[100];
    int r[100];
    int i,j,n;
    printf("enter sizef array");
    scanf("%d",&n);
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    i=0;
     for(j=0;j<n;j++){
         r[j]=a[i];
         i++;
    }
    for(j=0;j<n;j++){
        printf("%d",r[j]);
    }
    Return 0
}
